module Transpile

imports

  nabl2/Menus
  nabl2/References


menus
  menu: "Transpile" (openeditor)
    action: "Transpile  (.css.aterm)"  = transpile-interpret
    action: "Transpile  (.css) (permissive)" = transpile-print
    // This will only succeed if there are no analysis errors
    action: "Transpile  (.css)"        = transpile-print-strict
